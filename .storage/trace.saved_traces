              {
                "service_template": "switch.turn_{{ preferred_switch_state }}",
                "data": {
                  "entity_id": "{{ switch_entity_id }}"
                }
              },
              {
                "service": "saver.set_variable",
                "data": {
                  "name": "{{ switch_entity_id }}_last_switch",
                  "value": "{{ now() }}"
                }
              }
            ]
