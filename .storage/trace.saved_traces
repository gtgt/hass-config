              {
                "condition": "template",
                "value_template": "{{ ( as_timestamp(now()) - as_timestamp(state_attr('saver.saver', 'variables')[switch_entity_id + '_last_switch']|default('0')) | default(0, 'None') ) | int >= 300 }}"
              },
              {
